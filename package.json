{
  "name": "@microsoft/office-js-helpers",
  "description": "An open-source collection of helpers for developing Office Add-ins",
  "version": "0.2.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OfficeDev/office-js-helpers.git"
  },
  "engines": {
    "node": "6.3.0"
  },
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "umdify": "node build/browserify.js",
    "minify": "node build/minify.js",
    "clean": "rimraf dist && rimraf dts",
    "postinstall": "typings install",
    "build": "npm run clean && npm run tsc && npm run umdify && npm run minify",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags",
    "publish-patch": "npm version patch && npm publish",
    "publish-minor": "npm version patch && npm publish",
    "publish-beta": "npm publish --tag beta"
  },
  "main": "./dist/index.js",
  "jsnext:main": "./dist/index.js",
  "typings": "./dts/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/",
    "dts/"
  ],
  "dependencies": {
    "core-js": "^2.4.1"
  },
  "devDependencies": {
    "typings": "^1.4.0",
    "typescript": "^2.0.6",
    "rimraf": "^2.5.4",
    "tslint": "^3.15.1",
    "jasmine-core": "^2.5.2",
    "browserify": "^13.1.1",
    "uglify-js": "^2.7.4"
  },
  "keywords": [
    "Office Add-ins",
    "Office JavaScript API",
    "Authentication",
    "OAuth",
    "Storage",
    "Helpers"
  ],
  "author": "Microsoft",
  "bugs": {
    "url": "https://github.com/OfficeDev/office-js-helpers/issues"
  },
  "homepage": "https://github.com/OfficeDev/office-js-helpers#readme"
}
